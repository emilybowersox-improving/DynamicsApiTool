<div>
    <h1 class="display-4">Accounts</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Opportunity Id</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var opportunity in ViewBag.Data.value)
            {
                <tr>
                    <td><a href="/entity/opportunity/@opportunity["opportunityid"]">@opportunity["opportunityid"]</a></td>
                    <td>@opportunity["name"]</td>
                </tr>
            }
        </tbody>
    </table>

</div>