@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h1 class="display-4">Campaigns</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Campaign Id</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var campaign in ViewBag.Data.value)
            {
                <tr>
                    <td><a href="/entity/campaign/@campaign["campaignid"]">@campaign["campaignid"]</a></td>
                    <td>@campaign["name"]</td>
                </tr>
            }
        </tbody>
    </table>

</div>
