@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h1 class="display-4">Phone Calls</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Phone Call Id</th>
                <th>Subject</th>
                <th>Left Voicemail</th>
            </tr>
        </thead>
        <tbody>
           @foreach (var phoneCall in ViewBag.Data.value)
           {
                <tr>
                    <td><a href="/entity/phonecall/@phoneCall["activityid"]">@phoneCall["activityid"]</a></td>
                    <td>@phoneCall["subject"]</td>
                    <td>@phoneCall["leftvoicemail"]</td>
                </tr>
            }
        </tbody>
    </table>

</div>

