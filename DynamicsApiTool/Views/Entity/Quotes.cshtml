@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h1 class="display-4">Quotes</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Quote Id</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var quote in ViewBag.Data.value)
        {
        <tr>
            <td><a href="/entity/quote/@quote["quoteid"]">@quote["quoteid"]</a></td>
            <td>@quote["name"]</td>
        </tr>
        }
        </tbody>
    </table>
</div>