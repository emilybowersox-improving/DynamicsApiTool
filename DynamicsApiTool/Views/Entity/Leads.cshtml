@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h1 class="display-4">Leads</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Lead ID</th>
                <th>Company</th>
                <th>subject</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var lead in ViewBag.Data.value)
        {
            <tr>
                
                <td><a href="/entity/lead/@lead["leadid"]">@lead["leadid"]</a></td>
                <td>@lead["companyname"]</td>
                <td>@lead["subject"]</td>
            </tr>
        }
        </tbody>
    </table>

</div>
